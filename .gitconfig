[remote "TelegramMessenger"]
    url = git@github.com:TelegramMessenger/Telegram-iOS.git
    fetch = +refs/heads/*:refs/remotes/TelegramMessenger/*
[remote "origin"]
    url = git@github.com:huangyingw/TelegramMessenger_Telegram-iOS.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = TelegramMessenger
    branch = master
[merge]
    allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
[deploy]
	host = localhost
	path = ~/myproject/git/ossn/TelegramMessenger/Telegram-iOS
